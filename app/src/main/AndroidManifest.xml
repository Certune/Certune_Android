<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techtown.tarsosdsp_pitchdetect">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TarsosDSP_PitchDetect">
        <activity
                android:name=".ScrollTestActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Singing.activity.SingingTableActivity"
            android:exported="false" />
        <activity
            android:name=".Singing.activity.SingingStandbyActivity"
            android:exported="true" />
        <activity
            android:name=".SongListActivity"
            android:exported="true" />
        <activity
            android:name=".OctaveTest.activity.TestEndActivity"
            android:exported="true" />
        <activity
            android:name=".Login.RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".Singing.activity.SingingActivity"
            android:exported="true" />
        <activity
            android:name=".OctaveTest.activity.TestStandbyActivity"
            android:exported="true" />
        <activity
            android:name=".OctaveTest.activity.TestSingingActivity"
            android:exported="true" />
        <activity
            android:name=".Login.LogInActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>